// This is common TypeScript config for WebStorm (and other JetBrains products) for correct work of linter
{
  "compilerOptions": {
    "moduleResolution": "Node",
    "jsx": "react",
    "resolveJsonModule": true,
    "target": "ES6"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "packages/**/*.ts"
  ]
}
